{
	"manifest_version": 3,
	"key": "",
	"name": "Proton VPN: Fast & Secure",
	"description": "Secure your internet and protect your online privacy in one click.",
	"version": "1.2.3",
	"minimum_chrome_version": "108.0",
	"icons": {
		"16": "img/icon-16.png",
		"19": "img/icon-19.png",
		"24": "img/icon-24.png",
		"32": "img/icon-32.png",
		"38": "img/icon-38.png",
		"48": "img/icon-48.png",
		"64": "img/icon-64.png",
		"72": "img/icon-72.png",
		"96": "img/icon-96.png",
		"128": "img/icon-128.png",
		"256": "img/icon-256.png",
		"512": "img/icon-512.png"
	},
	"background": {
		"service_worker": "js/service.js"
	},
	"action": {
		"browser_style": false,
		"default_icon": {
			"16": "img/icon-16.png",
			"19": "img/icon-19.png",
			"24": "img/icon-24.png",
			"32": "img/icon-32.png",
			"38": "img/icon-38.png",
			"48": "img/icon-48.png",
			"64": "img/icon-64.png",
			"72": "img/icon-72.png",
			"96": "img/icon-96.png",
			"128": "img/icon-128.png",
			"256": "img/icon-256.png",
			"512": "img/icon-512.png"
		},
		"default_title": "Proton VPN",
		"default_popup": "popup.html"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'"
	},
	"host_permissions": [
		"https://*/",
		"http://*/"
	],
	"permissions": [
		"idle",
		"notifications",
		"privacy",
		"proxy",
		"scripting",
		"storage",
		"tabs",
		"webRequest",
		"webRequestAuthProvider"
	],
	"externally_connectable": {
		"matches": [
			"https://account.proton.me/*"
		]
	},
	"web_accessible_resources": [
		{
			"resources": [
				"js/*",
				"img/*",
				"css/*",
				"fonts/*"
			],
			"matches": []
		}
	]
}
